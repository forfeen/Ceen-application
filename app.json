{
  "expo": {
    "name": "ceenApp",
    "slug": "ceenApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "src/assets/images/splash_cus.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D9CDB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ske16.ceen",
      "config": {
        "googleMapsApiKey": "${process.env.iOSMapsApiKey}"
      }
      
    },
    "android": {
      "package": "com.ske16.ceen",
      "config" : {
        "googleMaps": {
        "apiKey": "${process.env.iOSMapsApiKey}"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
